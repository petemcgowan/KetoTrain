{
  "name": "keto-limit-server",
  "version": "1.0.0",
  "description": "Backend for Keto Train",
  "main": "server.mjs",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "universe/native",
    "parserOptions": {
      "es6": true
    },
    "env": {
      "es6": true
    }
  },
  "scripts": {
    "redisTest": "nodemon redisTest",
    "dockerServer": "docker compose up -d",
    "dockerDown": "docker compose down",
    "server": "nodemon server.mjs",
    "serverPreApr18Pt2": "node --loader ts-node/esm -r ts-node/register server.mjs",
    "serverPreApr18": "ts-node-esm server.mjs",
    "start": "babel-node server.ts --extensions \".ts\"",
    "server-babel": "babel-node server.ts --extensions \".ts\"",
    "server-ts": "node --loader ts-node/esm ./server",
    "server-ts-nodemon": "nodemon ./server.ts",
    "client": "npm start --prefix client",
    "devServerOnly": "npm run server"
  },
  "author": "Pete McGowan",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@apollo/server": "^4.6.0",
    "@google/generative-ai": "^0.24.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.14",
    "@types/strong-error-handler": "^2.3.0",
    "@types/validator": "^13.6.3",
    "apollo-server-express": "^3.12.0",
    "body-parser": "^1.20.2",
    "concurrently": "^6.3.0",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.6.0",
    "pg": "^8.0.3",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "8.0.6",
    "babel-preset-env": "^1.7.0",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.13",
    "strong-error-handler": "^3.5.0",
    "ts-node": "^10.3.0"
  },
  "nodemonConfig": {
    "delay": 3000,
    "ignore": [
      "client/*",
      "json/*"
    ]
  }
}
